{
  "name": "obsidian_plugin_for_todo_flow",
  "version": "1.2.32",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && mv main.css styles.css && npm run copy",
    "copy": "./sync_all.sh",
    "copy-to": "mkdir -p \"$VAULT/.obsidian/plugins/todo-flow\" && cp main.js styles.css manifest.json \"$VAULT/.obsidian/plugins/todo-flow/\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:quick": "vitest run src/__tests__/golden_scheduler.test.ts && npm run e2e -- --spec tests/e2e/journeys/*.spec.ts",
    "test:full": "npm run test && npm run e2e",
    "ship": "./ship.sh",
    "refresh": "./refresh.sh",
    "prepare": "husky",
    "e2e:setup": "npx tsx tests/e2e/setup-vault.ts",
    "e2e": "npm run e2e:setup && wdio run wdio.conf.mts",
    "test:flaky": "npm run e2e:setup && wdio run wdio.conf.mts --spec tests/e2e/drill-down.spec.ts tests/e2e/bug_007_verify.spec.ts tests/e2e/selective_flush.spec.ts",
    "e2e:watch": "wdio run wdio.conf.mts --watch",
    "e2e:open": "npm run e2e:setup && wdio run wdio.conf.mts --spec tests/e2e/manual-open.spec.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.3.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.10",
    "@wdio/cli": "^9.23.2",
    "@wdio/globals": "^9.23.0",
    "@wdio/local-runner": "^9.23.2",
    "@wdio/mocha-framework": "^9.23.2",
    "@wdio/spec-reporter": "^9.23.2",
    "esbuild": "^0.27.2",
    "esbuild-svelte": "^0.9.4",
    "happy-dom": "^20.3.7",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "moment": "^2.30.1",
    "obsidian": "^1.11.4",
    "svelte": "^5.48.2",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vitest-dom": "^0.1.1",
    "wdio-obsidian-reporter": "^2.2.1",
    "wdio-obsidian-service": "^2.2.1"
  }
}
